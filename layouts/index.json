[
    {{- range $index, $page :=.Site.RegularPages -}}
        {{- $months := index .Site.Data (i18n "months") -}}
        {{- $month := index $months (printf "%d" .PublishDate.Month) -}}
        {{- if ne $index 0 -}},{{- end -}}
        {
            'title': '{{ .Title }}',
            'link': '{{ .Permalink }}',
            'date': '{{ i18n "published" (dict "Day" .PublishDate.Day "Month" $month "Year" .PublishDate.Year) }}'
        }
    {{- end -}}
]